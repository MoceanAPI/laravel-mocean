{
    "name": "mocean/laravel-mocean",
    "description": "Mocean API for Laravel",
    "type": "library",
    "keywords": ["sms","verify","laravel","mocean"],
    "license": "MIT",
    "require": {
        "php": ">=7.3 <8",
        "mocean/client7": "^2.0.0",
        "illuminate/support": "~4||~5||~6||~7||~8||~9"
    },
    "require-dev": {
        "illuminate/console": "~4||~5||^6.0",
        "phpunit/phpunit": "~6.0 || ~7.0 || ~8.0 || ~9.0",
        "orchestra/testbench": "~3.0 || ~4.0"
    },
    "autoload": {
        "psr-4": {
            "Mocean\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mocean\\Laravel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mocean\\Laravel\\MoceanServiceProvider"
            ],
            "aliases": {
                "Mocean": "Mocean\\Laravel\\Facade\\Facade"
            }
        }
    }
}
