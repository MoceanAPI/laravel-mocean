{
    "name": "mocean/laravel-mocean",
    "description": "Mocean API for Laravel",
    "type": "library",
    "keywords": ["sms","verify","laravel","mocean"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "mocean/client": "^2.0.0",
        "illuminate/support": "~4||~5||^6.0"
    },
    "require-dev": {
        "illuminate/console": "~4||~5||^6.0",
        "phpunit/phpunit": "~4.5 || ~8.0 || ~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Mocean\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mocean\\Laravel\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mocean\\Laravel\\MoceanServiceProvider"
            ],
            "aliases": {
                "Mocean": "Mocean\\Laravel\\Facade\\Facade"
            }
        }
    }
}
